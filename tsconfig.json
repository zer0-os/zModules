{
  "compilerOptions": {
    "target": "es2020",
	"lib": [
		"es5", "es6", "dom", "dom.iterable"
	],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "./dist",
  }
}
